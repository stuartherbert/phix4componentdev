<?xml version="1.0" encoding="UTF-8"?>
<package xmlns="http://pear.php.net/dtd/package-2.0" xmlns:tasks="http://pear.php.net/dtd/tasks-1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" packagerversion="1.9.1" version="2.0" xsi:schemaLocation="http://pear.php.net/dtd/tasks-1.0     http://pear.php.net/dtd/tasks-1.0.xsd     http://pear.php.net/dtd/package-2.0     http://pear.php.net/dtd/package-2.0.xsd">
  <name>${project.name}</name>
  <channel>${project.channel}</channel>
  <summary>phix and all dependencies required for creating and maintaining PHP components</summary>
  <description>
    phix4componentdev is a meta-package that installs phix itself, plus all of the phix extensions required for creating and maintaining PHP components, plus all of the third-party tools you'll need to use.
  </description>
  <lead>
    <name>Stuart Herbert</name>
    <user>stuart</user>
    <email>stuart@stuartherbert.com</email>
    <active>yes</active>
  </lead>
  <date>${build.date}</date>
  <time>${build.time}</time>
  <version>
  <release>${project.version}</release>
    <api>${project.majorVersion}.${project.minorVersion}</api>
  </version>
  <stability>
    <release>${project.stability}</release>
    <api>stable</api>
  </stability>
  <license>New BSD license.</license>
  <notes>
    No notes.
  </notes>
  <contents>
    <dir baseinstalldir="/" name="/">
${contents}
    </dir>
  </contents>
  <dependencies>
    <required>
      <php>
        <min>5.3.0</min>
      </php>
      <pearinstaller>
        <min>1.9.4</min>
      </pearinstaller>
      <package>
        <name>ComponentManagerPhpLibrary</name>
        <channel>pear.phix-project.org</channel>
        <min>10.0.0snapshot</min>
	      <max>10.999.9999</max>
      </package>
      <package>
        <name>PHPUnit</name>
        <channel>pear.phpunit.de</channel>
        <min>3.5.0</min>
        <max>3.999.9999</max>
      </package>
      <package>
        <name>phing</name>
        <channel>pear.phing.info</channel>
        <min>2.4.6</min>
        <max>2.999.9999</max>
      </package>
      <package>
        <name>PHP_Depend</name>
        <channel>pear.pdepend.org</channel>
        <min>0.10.5</min>
        <max>1.999.9999</max>
      </package>
      <package>
        <name>PHP_PMD</name>
        <channel>pear.phpmd.org</channel>
        <min>1.1.1</min>
        <max>1.999.9999</max>
      </package>
      <package>
        <name>PhpDocumentor</name>
        <channel>pear.php.net</channel>
        <min>1.4.3</min>
        <max>1.999.9999</max>
      </package>
      <package>
        <name>phpcpd</name>
        <channel>pear.phpunit.de</channel>
        <min>1.3.2</min>
        <max>1.999.9999</max>
      </package>
      <package>
        <name>PHP_CodeSniffer</name>
        <channel>pear.php.net</channel>
        <min>1.3.0</min>
        <max>1.999.9999</max>
      </package>
      <package>
        <name>PHP_CodeBrowser</name>
        <channel>pear.phpunit.de</channel>
        <min>1.0.0</min>
        <max>1.999.9999</max>
      </package>
      <package>
        <name>Pirum</name>
        <channel>pear.pirum-project.org</channel>
        <min>1.0.4</min>
        <max>1.999.9999</max>
      </package>
      <package>
        <name>phix</name>
        <channel>pear.phix-project.org</channel>
        <min>${project.majorVersion}.${project.minorVersion}.${project.patchLevel}snapshot</min>
        <max>${project.majorVersion}.${project.minorVersion}.${project.patchLevel}</max>
      </package>
    </required>
  </dependencies>
  <phprelease/>
  <changelog>
    <release>
      <version>
        <release>0.13.0</release>
        <api>0.13</api>
      </version>
      <stability>
        <release>stable</release>
        <api>stable</api>
      </stability>
      <date>2011-08-19</date>
      <license>New BSD license</license>
      <notes>
      </notes>
    </release>
  </changelog>
</package>
